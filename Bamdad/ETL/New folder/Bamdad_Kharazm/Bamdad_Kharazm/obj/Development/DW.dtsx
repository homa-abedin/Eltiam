            <SQLTask:SqlTaskData
              SQLTask:Connection="{3810A15B-64DA-40D3-9826-B8CC9CADDE42}"
              SQLTask:SqlStatementSource="drop table if exists dw..titaccustomer&#xA;Go&#xA;select ROW_NUMBER() OVER(ORDER BY  a.GLN ASC) AS  idtitac ,a.*&#xA;into dw..titaccustomer&#xA;&#xA; from (&#xA;select &#xA; distinct p.gln&#xA;--, p.Name javazname&#xA;, p.OwnerName&#xA;, p.Provice&#xA;, p.Latitude&#xA;, p.Longitude&#xA;, customer.customerkey&#xA;,case when  customer.name is not null then customer.name   else p.Name end as   customername&#xA;, customer.centerid&#xA;--, case when customer.centerid is null  then dc.id when dc.centerkey =1 then 1&#xA;--&#x9;&#x9;when dc.centerkey=2 then 2 else  customer.centerid end as newcenter&#xA;,case when customer.customerkey is null then 0 else 1 end as mycustomer&#xA;&#xA;, case when p.Provice like N'تهران' and customer.centerid is null then 1 when customer.centerid is null  then dc.id &#xA;&#x9;&#x9; else  customer.centerid end as newcenter&#xA;from  eltiam.[Pharmacies] p &#xA;&#xA; full join (select cd.id as customerkey ,name, GLN,CustomerTypeId, PharmacyTypeId, HIX,center.id as centerid &#xA;&#x9;&#x9;&#x9;&#x9;from  eltiam.PUBCustomerDetails cd&#xA;&#x9;&#x9;&#x9;join eltiam.PUBCustomers c on  c.Id=cd.Id&#xA;&#x9;&#x9;&#x9;left join (select * from dw..dimcenter where mycenter=1 )center on center.centerkey=c.CenterKey&#xA;&#x9;&#x9;&#x9;where Status=1 and IsDeactivated=0 and gln is not null&#xA;&#x9;&#x9;&#x9;)customer on p.GLn=customer.GLN &#xA;left join (select * from dw..dimcenter) dc &#xA;&#x9;on dc.Provincename = case when  p.Provice like N'چهارمحال و بختیاری' then N'چهارمحال بختیاری' &#xA;&#x9;when  p.Provice like N'کهگیلویه و بویراحمد' then N'کهکیلویه و بویر احمد' else p.Provice end&#xA;where p.Latitude  between 20 and 60  &#xA;or p.Longitude  between 30 and 90) a" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
