                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--truncate table dw..factsales
 --drop table dw..factsales

SELECT  d.Id AS DetailId,
	   H.Id as HeaderId,
       d.ProductKey,
	   h.OrderDateKey AS DateKey,
	   --h.InvoiceDateKey InvoiceDateKey,
	   center.id as centerID,
	   h.CustomerKey,
	   h.SalesmanKey,
	   H.InvoiceNumber,
	   H.OrderNumber,
	   case when h.LastStatusKey&gt;=5 then D.Quantity else 0 end as Quantity,
	   case when h.LastStatusKey&gt;=5 then isnull(D.DiscountQuantity,0) else 0 end as DiscountQuantity,
	   case when h.LastStatusKey&gt;=5 then isnull(D.DiscountAmount,0) else 0 end as DiscountAmount,
	   case when h.LastStatusKey&gt;=5 then d.OrderedQuantity-D.Quantity else 0 end as LostSalesQuantity,
	   case when h.LastStatusKey&lt;5 then D.Quantity else 0 end as NIQuantity, 
	   case when h.LastStatusKey&lt;5 then isnull(D.DiscountQuantity,0) else 0 end as NIDiscountQuantity,
	   D.DueDateDuration+isnull(h.InvoiceSpecialDDDIncrease,0) DueDateDuration,
	   D.Cost,
	   D.Price,
	   D.ConsumerPrice,
	   CEILING((d.Quantity*d.Price)*D.VAT) as VAT,
	   isnull(CEILING((d.DiscountQuantity*d.Price)*D.DiscountQuantityVAT),0) as DiscountQuantityVAT,
	   h.ShipDateKey,
	   H.InvoiceDueDate as DueDateKey,
	   H.InvoiceNetAmount AS InvoiceNetAmount,

	   D.VAT AS VATPerc,
       isnull(h.InvoiceSpecialDDDIncrease,0) InvoiceSpecialDDDIncrease,
	   --s.id as StatusSetid,
	   --cast(replace(CONVERT(VARCHAR(5) ,case when cast(d.Created as date)!=cast(s.Created as date) then s.Created else D.Created end,108),':','') as int) DetailCreatedKey
	    1 as company
	   ,isnull (rd.returnQuantity,0)returnQuantity
	    , isnull (rd.DiscountQuantity,0) returnDiscountQuantity, isnull (rd.DiscountAmount,0) returnDiscountAmount
	   , isnull (rd.Cost,0) returnCost , isnull (rd.Price,0) returnPrice, isnull (rd.ConsumerPrice,0) returnConsumerPrice 
	   ,rd.reasonlookup
	   , ddds.DueDateDuration  MainDueDateDuration
	   --into DW..FactSales
	   --select * 
from  eltiam.SLSSalesOrderDetails D
LEFT JOIN eltiam.SLSSalesOrderHeaders H
	ON H.Id = D.SalesOrderHeaderKey
left join eltiam.SLSSalesOrderStatusSet s on s.SalesOrderHeaderKey=h.Id and s.SalesOrderStatusKey=5 
left join (

select  sum(Quantity) returnQuantity, avg(Price)Price, avg (Cost)Cost,avg(ConsumerPrice)ConsumerPrice, SalesOrderDetailKey,ProductKey,sum(DiscountQuantity)DiscountQuantity,sum(DiscountAmount)DiscountAmount,max(reasonlookup)reasonlookup from 
	(
	select Quantity, Price, Cost,ConsumerPrice,DiscountQuantity, SalesOrderDetailKey,ProductKey,SalesReturnHeaderKey,SalesOrderHeaderKey ,rh.ReasonLookup,DiscountAmount
				from Eltiam.SLSSalesReturnDetails  rd
				left join (select * from Eltiam.SLSSalesReturnHeaders where IsApproved=1) rh on  rh.id=rd.SalesReturnHeaderKey 
				--where rd.SalesOrderDetailKey=14026219
		)a
group by   SalesOrderDetailKey,ProductKey

	) rd on  rd.SalesOrderDetailKey=d.Id  
join (select * from dw..dimcenter where mycenter=1)center on center.centerkey=h.CenterKey 
left join (select * from stage.Eltiam.PUBProductDDDs)ddds on d.ProductKey= ddds.ProductKey and h.InvoiceDateKey between ddds.StartDateKey and ddds.EndDateKey
where
--h.InvoiceDateKey&gt;=14030101  --and d.id=16547410
H.Id  is not null  
and (d.isinsert=1
or h.isinsert=1 ) --or s.isinsert=1</property>
