              SQLTask:SqlStatementSource="drop table if exists dw..demand&#xA;select * into dw..Demand from (&#xA;&#xA;SELECT&#x9;&#xA;&#x9;DISTINCT N'سررسیدشده' AS typechild&#xA;&#x9;, N'فاکتور' AS typeparent&#xA;&#x9;,s.Id,331 AS typeId&#xA;&#x9;,c.id as centerid&#xA;&#x9;,CAST(s.InvoiceNumber AS NVARCHAR(20)) AS Number&#xA;&#x9;,s.CustomerKey &#xA;&#x9;,d.datekey AS dateKey&#xA;&#x9;,s.RemainAmount AS remain&#xA;&#xA;FROM &#xA;&#x9;eltiam .SLSSalesOrderHeaders AS s&#xA;&#x9;left join (select * from dw..dimcenter where mycenter=1)c on c.centerkey=s.WarehouseKey&#xA;&#x9;join (select * from dimdate)d on d.Gregorian=convert (date ,s.Created )&#xA;WHERE &#xA;&#x9;RemainAmount &lt;&gt; 0&#xA;&#x9;and s.InvoiceDueDate&lt;=(select DateKey from DimDate where Gregorian= convert (date ,getdate()   ) )&#xA;&#xA;union &#xA;SELECT&#x9;&#xA;&#x9;DISTINCT N'سررسید نشده' AS typechild&#xA;&#x9;, N'فاکتور' AS typeparent&#xA;&#x9;,s.Id,331 AS typeId&#xA;&#x9;,c.id as centerid&#xA;&#x9;,CAST(s.InvoiceNumber AS NVARCHAR(20)) AS Number&#xA;&#x9;,s.CustomerKey &#xA;&#x9;,d.datekey AS dateKey&#xA;&#x9;,s.RemainAmount AS remain&#xA;&#xA;FROM &#xA;&#x9;eltiam .SLSSalesOrderHeaders AS s&#xA;&#x9;left join (select * from dw..dimcenter where mycenter=1)c on c.centerkey=s.WarehouseKey&#xA;&#x9;join (select * from dimdate)d on d.Gregorian=convert (date ,s.Created )&#xA;WHERE &#xA;&#x9;RemainAmount &lt;&gt; 0&#xA;&#x9;and s.InvoiceDueDate&gt;(select DateKey from DimDate where Gregorian= convert (date ,getdate()   )   )&#xA;&#xA;union &#xA;SELECT&#xA;&#x9;DISTINCT N'حقوقی' AS typechild&#xA;&#x9;&#x9;, N'فاکتور' AS typeparent&#xA;&#xA;&#x9;,s.Id,331 AS typeId&#xA;&#x9;,c.id as centerid&#xA;&#x9;,CAST(s.InvoiceNumber AS NVARCHAR(20)) AS Number&#xA;&#x9;,s.CustomerKey &#xA;&#x9;,d.datekey AS dateKey&#xA;&#x9;,s.RemainAmount AS remain&#xA;&#xA;FROM &#xA;&#x9;eltiam .SLSSalesOrderHeaders AS s&#xA;&#x9;JOIN eltiam.LegalCheques AS a ON s.id = a.debitid&#xA;&#x9;left join (select * from dw..dimcenter where mycenter=1)c on c.centerkey=s.WarehouseKey&#xA;&#x9;join (select * from dimdate)d on d.Gregorian=convert (date ,s.Created )&#xA;&#xA;WHERE &#xA;&#x9;RemainAmount &lt;&gt; 0&#xA;&#x9;and a.typeid=331&#xA;union&#xA;SELECT&#x9;&#xA;&#x9;DISTINCT N'برگشتی' AS typechild&#xA;&#x9;, N'چک' AS typeparent&#xA;&#x9;,r.id&#xA;&#x9;,332 AS typeId&#xA;&#x9;,c.id as centerid&#xA;&#x9;, r.Number&#xA;&#x9;, r.customerkey&#xA;&#x9;,d.datekey AS dateKey&#xA;&#x9;, r.RemainAmountIfBounced AS remain &#xA;FROM&#x9;&#xA;&#x9;eltiam.ARPReceivables AS r&#xA;&#x9;left join (select * from dw..dimcenter where mycenter=1)c on c.centerkey=r.centerkey&#xA;&#x9;join (select * from dimdate)d on d.Gregorian=convert (date ,r.Created )&#xA;&#xA;WHERE &#xA;&#x9;r.PaymentMethodLookup=1 and &#xA;&#x9;LastStatusLookup = 3 &#xA;&#x9;AND RemainAmountIfBounced &gt; 0&#xA;&#x9;and r.ChequeDateKey&lt;=(select DateKey from DimDate where Gregorian= convert (date ,getdate()   ))&#xA;&#xA;&#x9;union&#xA;&#xA;&#xA;SELECT&#x9;&#xA;&#x9;DISTINCT N'حقوقی' AS typechild&#xA;&#x9;, N'چک' AS typeparent&#xA;&#x9;,r.id&#xA;&#x9;,332 AS typeId&#xA;&#x9;,c.id as centerid&#xA;&#x9;, r.Number&#xA;&#x9;, r.customerkey&#xA;&#x9;,d.datekey AS dateKey&#xA;&#x9;, r.RemainAmountIfBounced AS remain &#xA;&#x9;&#xA;FROM&#x9;&#xA;&#x9;eltiam.ARPReceivables AS r&#xA;&#x9;JOIN eltiam.LegalCheques AS a ON r.id = a.debitid&#xA;&#x9;left join (select * from dw..dimcenter where mycenter=1)c on c.centerkey=r.centerkey&#xA;&#x9;join (select * from dimdate)d on d.Gregorian=convert (date ,r.Created )&#xA;WHERE &#xA;&#x9;r.PaymentMethodLookup=1 and &#xA;&#x9; RemainAmountIfBounced &gt; 0&#xA;&#x9;and a.typeid=332&#xA;union&#xA;SELECT&#x9;&#xA;&#x9;DISTINCT N'سررسیدشده' AS typechild&#xA;&#x9;, N'چک' AS typeparent&#xA;&#x9;,r.id&#xA;&#x9;,332 AS typeId&#xA;&#x9;,c.id as centerid&#xA;&#x9;, r.Number&#xA;&#x9;, r.customerkey&#xA;&#x9;, d.datekey AS dateKey&#xA;&#x9;, r.Amount AS remain&#xA;&#x9;&#xA;FROM&#x9;&#xA;&#x9;eltiam.ARPReceivables AS r&#xA