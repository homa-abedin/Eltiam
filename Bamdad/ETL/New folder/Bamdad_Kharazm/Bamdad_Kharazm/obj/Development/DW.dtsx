                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--truncate table dw..factsales

SELECT  d.Id AS DetailId,
	   H.Id as HeaderId,
       d.ProductKey,
	   h.OrderDateKey AS DateKey,
	   H.CenterKey,
	   h.CustomerKey,
	   h.SalesmanKey,
	   H.InvoiceNumber,
	   H.OrderNumber,
	   case when h.LastStatusKey&gt;=5 then D.Quantity else 0 end as Quantity,
	   case when h.LastStatusKey&gt;=5 then isnull(D.DiscountQuantity,0) else 0 end as DiscountQuantity,
	   case when h.LastStatusKey&gt;=5 then isnull(D.DiscountAmount,0) else 0 end as DiscountAmount,
	   case when h.LastStatusKey&gt;=5 then d.OrderedQuantity-D.Quantity else 0 end as LostSalesQuantity,
	   case when h.LastStatusKey&lt;5 then D.Quantity else 0 end as NIQuantity, 
	   case when h.LastStatusKey&lt;5 then isnull(D.DiscountQuantity,0) else 0 end as NIDiscountQuantity,
	   D.DueDateDuration+isnull(h.InvoiceSpecialDDDIncrease,0) DueDateDuration,
	   D.Cost,
	   D.Price,
	   D.ConsumerPrice,
	   CEILING((d.Quantity*d.Price)*D.VAT) as VAT,
	   isnull(CEILING((d.DiscountQuantity*d.Price)*D.DiscountQuantityVAT),0) as DiscountQuantityVAT,
	   h.ShipDateKey,
	   H.InvoiceDueDate as DueDateKey,
	   H.InvoiceNetAmount AS InvoiceNetAmount,

	   D.VAT AS VATPerc,
       isnull(h.InvoiceSpecialDDDIncrease,0) InvoiceSpecialDDDIncrease,
	   s.id as StatusSetid,
	   cast(replace(CONVERT(VARCHAR(5) ,case when cast(d.Created as date)!=cast(s.Created as date) then s.Created else D.Created end,108),':','') as int) DetailCreatedKey
	   , 1 as company
	   ,sku.Id as OrderDetailSKUsid
	   ,rds.id as ReturnDetailSKUsid
	   ,ret.ReturnDetailsid  
	   ,ret.returnheaderid
	   ,ret.returnTotalQuantity, ret.returnquantity, ret.returnDiscountQuantity, ret.returnDiscountAmount
	   , ret.returnCost , ret.returnPrice, ret.returnConsumerPrice , ret.ReasonLookup , ret.Created
	   ,ret.ApprovedDateKey
	   ,whssku.id skukey, whssku.BatchNumber, LookupPurchaseType ,rds.Created returnDate

,case when (left(batchnumber,1)='#' and LookupPurchaseType=1) then N'سیرفروش' when (  LookupPurchaseType =1 and left(batchnumber,1)!= '#') then N'قطعی' when LookupPurchaseType=0 then N'امانی' end as kindsales
 , whssku.ExpireAlternateDateKey, HaveGTIN_B, HaveIRC_B

	 --into DW..FactSalesBatch
from  eltiam.SLSSalesOrderDetailSKUs sku
left join (select * from eltiam.SLSSalesOrderDetails)d on sku.SalesOrderDetailKey=d.id
LEFT JOIN eltiam.SLSSalesOrderHeaders H 	ON H.Id = D.SalesOrderHeaderKey
left join eltiam.SLSSalesOrderStatusSet s on s.SalesOrderHeaderKey=h.Id and s.SalesOrderStatusKey=5 
left join (
		 select rh.Id as returnheaderid ,rd.Id  as ReturnDetailsid, rd.SalesOrderDetailKey, rh.ApprovedDateKey ,rh.CenterKey

	   ,rd.TotalQuantity returnTotalQuantity, rd.Quantity returnquantity, rd.DiscountQuantity returnDiscountQuantity, rd.DiscountAmount returnDiscountAmount
	   , rd.Cost returnCost , rd.Price returnPrice, rd.ConsumerPrice returnConsumerPrice , rh.ReasonLookup , rh.Created
	    

		 from Eltiam.SLSSalesReturnHeaders  rh
		left join (select * from Eltiam.SLSSalesReturnDetails ) rd on   rd.SalesReturnHeaderKey=rh.id
			where IsApproved=1  and rh.id=153620
		)ret  on ret.SalesOrderDetailKey=d.Id
left join (select * from Eltiam.SLSSalesReturnDetailSKUs) rds on rds.SalesReturnDetailKey=ret.ReturnDetailsid
left join (select * from eltiam.WHSSKUs)whssku on whssku.Id= sku.SKUKey and whssku.WHSSKU_PUBProduct= d.ProductKey
where H.Id  is not null
and (whssku.isinsert=1
or sku.isinsert=1  )</property>
