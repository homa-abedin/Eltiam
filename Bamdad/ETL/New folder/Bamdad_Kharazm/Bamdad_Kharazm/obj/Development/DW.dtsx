                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--with CTE_Groups as (
--select p.id as productkey , DueDateDuration ,pr.Id as  priceID,dd.StartDateKey, dd.EndDateKey ,DDDID,sum(case when DueDateDuration is not null then 1 else 0 end ) over (order by p.id,pr.id, dd.StartDateKey)as gpid  

--FROM eltiam.PUBProducts P

--inner join (select IsVATable,id,Cost,Price,ConsumerPrice,PUBProductPrice_PUBProduct,StartDateKey,EndDateKey,IsConfirmed from eltiam.PUBProductPrices )pr on pr.PUBProductPrice_PUBProduct=p.id
--left join (select ProductKey,StartDateKey,EndDateKey,DueDateDuration,Id as DDDID  from eltiam.PUBProductDDDs where  IsConfirmed=1 ) dd on  dd.ProductKey=pr.PUBProductPrice_PUBProduct 
--	and dd.StartDateKey between pr.StartDateKey and pr.EndDateKey 
--group by p.Id  , DueDateDuration ,pr.Id,dd.StartDateKey, dd.EndDateKey, DDDID
--	)

--,
--CTE_filled as (

--select *,max(  DueDateDuration ) over (partition by gpid order by  priceID)as filled_column
--,max(  startdatekey ) over (partition by gpid order by  priceID)as filledstartdatekey
--,max(  EndDateKey ) over (partition by gpid order by  priceID)as filledenddatekey


--from CTE_Groups

--)

--drop table if exists dw..dimproduct
--GO

SELECT 

distinct P.Id AS ProductKey,
	     P.Code AS ProductCode,
         P.[Name] AS Product,
		cast( p.Created as date) DateCreate,
	   d.PUBProductDetail_PUBSupplier AS SupplierKey,
	   SU.Code AS SupplierCode,
	   SU.[Name] AS Supplier,
	   c.id as drugid,
	   ISNULL(C.DrugCode, D.DrugCode) AS JointKey,
	   C.BrandName AS EnglishProductName,
	   C.DrugShape,
	   CASE D.IsImported WHEN 0 THEN N'تولیدی' ELSE N'وارداتی' END AS ProductSupplyType,
	   t.id as typeid,
	   T.[Name] AS PRoductCategory,
	   sh.Id as shapeid,
	   SH.[Name] AS PersianShapeName,
	   SH.EnglishName AS EnglishShapeName,
	   CASE D.LookupPurchaseType WHEN 1 THEN N'قطعی' ELSE N'امانی' END AS PurchaseType,
	   D.LookupPurchaseType AS PurchaseTypeCode,
       p.[Status] AS ProductStatus,
	   PS.GroupId AS SimilarKey,
	   PS.GroupName AS Similar,
	   D.PackQuantity AS PackQuantity,
	   D.CartonQuantity AS CartonQuantity,
	   
	   --pr.Cost AS CostNow,
    --   pr.Price AS PriceNow,
	   --pr.ConsumerPrice AS ConsumerPriceNow,
	   --isnull(pr.IsVATable,0) AS IsVATableNow,

	   S.Id AS StoringConditionCode,
	   S.[Name] AS StoringCondition,
	   (CASE WHEN P.Id IN (SELECT ProductKey
						   FROM
							   (
								SELECT ROW_NUMBER() OVER (PARTITION BY ProductKey ORDER BY Id DESC) AS Row_Num, ProductKey, IsBlocked
								FROM eltiam.WHSSKUStatusSet
								WHERE (SKUKey IS NULL) AND (WarehouseKey IS NULL)
							   ) T
						   WHERE Row_Num = 1 AND IsBlocked = 1)
			 THEN N'بلوکه' ELSE N'آزاد'
	    END) AS BlockedStatus,
	   D.IsHospitalMedicine,
	   d.IRC
	   ,d.GTIN
	  --,pr.IsConfirmed 
	  --,kkk.filled_column AS RespiteNow
	  --,kkk.filledstartdatekey as strartdateDDD
	  --,kkk.filledenddatekey as enddateDD
	  --,pr.StartDateKey as startdateP
	  --,pr.EndDateKey as enddateP
	  --, kkk.DDDID
	  --,kkk.priceID
	  --,sku.*
  --into dw..dimproduct
 
FROM eltiam.PUBProducts P
LEFT JOIN (select * from  eltiam.PUBProductDetails) D
	ON P.Id = D.Id
LEFT JOIN (select * from eltiam.PUBProductDrugCodes) C
	ON C.DrugCode = D.DrugCode
LEFT JOIN (select id , Name from eltiam.PUBProductTypes) T
	ON T.Id = D.PUBProductDetail_PUBProductType
LEFT JOIN (select id,name,EnglishName from eltiam.PUBProductShapes) SH
	ON SH.Id = D.PUBProductDetail_PUBProductShape
LEFT JOIN (select id,Name from eltiam.PUBProductStoringConditions) S
	ON S.Id = D.PUBProductDetail_PUBProductStoringCondition
LEFT JOIN (select id, code,Name from eltiam.PUBSuppliers) SU
	ON SU.Id = D.PUBProductDetail_PUBSupplier
--left join (select * from eltiam.WHSSKUs)sku on sku.WHSSKU_PUBProduct = p.Id 


--inner join (select IsVATable,id,Cost,Price,ConsumerPrice,PUBProductPrice_PUBProduct,StartDateKey,EndDateKey,IsConfirmed from eltiam.PUBProductPrices )pr on pr.PUBProductPrice_PUBProduct=p.id

--left join (select priceID,DueDateDuration,gpid , filled_column,filledstartdatekey,filledenddatekey,productkey,DDDID
--			from CTE_filled) kkk on kkk.priceID=pr.Id  and kkk.productkey=p.Id 


LEFT JOIN
(
	SELECT PS.GroupId, SUBSTRING(P.[Name], 1, 15) GroupName, ProductKey
	FROM
	(
		SELECT GroupId, MIN(PUBSimilarProducts_PUBProduct) OVER (PARTITION BY GroupId) GroupNameCode, PUBSimilarProducts_PUBProduct AS ProductKey
		FROM eltiam.PUBSimilarProducts
) PS
INNER JOIN eltiam.PUBProducts P
	ON P.Id = PS.GroupNameCode
) PS
	ON PS.ProductKey = P.Id</property>
