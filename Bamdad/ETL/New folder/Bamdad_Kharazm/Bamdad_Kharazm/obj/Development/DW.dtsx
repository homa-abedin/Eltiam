                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select h.Id DocHeaderID, d.Id DetailID,l1.Id IDL1,  l2.Id IDL2, l3.Id  IDL3,d.FINDocDetail_FINDetailAccountL4 AccountL4
, d.FINDocDetail_FINDetailAccountL5 AccountL5, d.FINDocDetail_FINDetailAccountL6 AccountL6, Debit, Credit , h.FINDocHeader_FINDocType, DateKey
--into dw..FactFinance

from [Eltiam].FINDocHeaders h 
LEFT JOIN [Eltiam].FINDocDetails AS d ON h.Id = d.FINDocDetail_FINDocHeader
LEFT JOIN (select * from [Eltiam].FINL3Accounts) AS l3 ON l3.id = d.FINDocDetail_FINL3Account
LEFT JOIN (select * from [Eltiam].FINL2Accounts) AS l2 ON l2.Id = l3.FINAccountsL3_FINAccountsL2
LEFT JOIN (select * from [Eltiam].FINL1Accounts) AS l1 ON l1.Id = FINAccountsL2_FINAccountsL1
left join (SELECT *  FROM [Eltiam].FINDetailAccounts )f1 on f1.id= d.FINDocDetail_FINDetailAccountL4 
left join (SELECT *  FROM [Eltiam].FINDetailAccounts )f2 on f2.id= d.FINDocDetail_FINDetailAccountL5 
where h.isupdate=1
and d.isupdate=1</property>
