      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{B497DD00-C1C2-4B58-A27A-FCFA87D02C4B}"
          SQLTask:SqlStatementSource="drop table if exists  [DW].kharazmi.dimcustomer &#xA;&#xA;&#xA;&#xA; select &#xA;c.CustomerID&#xA;,c.Number&#xA;,ISNULL(d.DLID,c.number)DLID&#xA;,p.firstname&#xA;,p.lastname&#xA;, CASE &#xA;    WHEN FullName LIKE N'شرکت%' THEN &#xA;      LTRIM(SUBSTRING(FullName, 6, LEN(FullName)))&#xA;    ELSE FullName&#xA;  END AS namecustomer&#xA;--,case when right(FullName, 5) like N'شرکت' then  p.FullName  AS  namecustomer&#xA;,c.State ,p.partyid&#xA;,CAST (c.CreationDate AS DATE)CreationDate&#xA;,a.Latitude&#xA;,a.Longitude&#xA;&#xA;,a.RegionalDivisionRef&#xA;,a.Details&#xA;,d.Title as TitleDl&#xA;--,d.Code as CodeDL,ac.AccountID,ac.Name NameAccount,ac.Number NumberAccount&#xA;--,EntityGroupingID,  groupingname,  groupid, namegroup,  subgroupid,  namesubgroup &#xA;--,changedate,dd.idpersian createpersian,r.*&#xA;&#xA; into [DW].kharazmi.dimcustomer &#xA; from &#xA; kharazmi.customer c&#xA; left join (Select * from kharazmi.Party) p on p.PartyID=c.PartyRef&#xA; left join (Select * from  kharazmi.CustomerAddress &#xA; where Type=1 and IsActive=1  and isdefault=1 AND State = 1&#xA; ) ca on ca.CustomerRef=c.CustomerID&#xA; left join (Select * from kharazmi.Address  ) a on a.AddressID=ca.AddressRef&#xA; --left join (&#xA;&#x9;--&#x9;&#x9;select &#xA;&#x9;--&#x9;&#x9;--egi.EntityGroupingID, egi.name groupingname,&#xA;&#x9;--&#x9;&#x9;eg1.EntityGroupID groupid,EG1.Name namegroup, eg.EntityGroupID subgroupid, EG.Name namesubgroup--, gm.memberid  &#xA;&#x9;--&#x9;&#x9;from  kharazmi.EntityGroup EG &#xA;&#x9;--&#x9;&#x9;join (select * from kharazmi.EntityGroup)EG1 on EG1.EntityGroupID=isnull(EG.ParentRef,EG.EntityGroupID)&#xA;&#x9;--&#x9;&#x9;join (select * from kharazmi.EntityGrouping) egi on egi.EntityGroupingID=EG1.EntityGroupingRef&#xA;&#x9;--&#x9;&#x9;--join (select * from kharazmi.[CustomerGroupMembers])gm on gm.GroupRef=eg.EntityGroupID &#xA;&#x9;--&#x9;&#x9;--where  entityname like 'customer'&#xA;&#x9;--&#x9;&#x9;)category on category.MemberID=c.CustomerID&#xA;left join (select * from kharazmi.dl where DLTypeRef=1 )d on d.ReferenceID=p.partyid&#xA;--left join (select top(1)AccountID,name,Number,PartyRef from kharazmi.Account)ac on ac.PartyRef=p.PartyID&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
