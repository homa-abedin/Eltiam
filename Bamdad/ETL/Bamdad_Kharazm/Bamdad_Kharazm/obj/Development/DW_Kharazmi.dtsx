          SQLTask:SqlStatementSource="drop table if exists dw.kharazmi.[Target]&#xA;--select &#xA;--sum(amount), sum(cnt) from (&#xA;SELECT &#xA;    amount*ratio amount, [count]*ratio cnt, product,productcode&#xA;&#x9;, month,bd.code pakhshcode, bd.name pakhshname--,bc.cityid,bc.province, bc.center &#xA;&#xA;&#xA;&#x9;    --amount*ratio*isnull(budget,0)amount, [count]*ratio*isnull(budget,0)cnt, product,productcode&#xA;&#xA;&#x9;into dw.kharazmi.[Target]&#xA;--FROM [Stage].[kharazmi].[budgetcenter] bc&#xA;FROM [Stage].[kharazmi].[budgetpakhsh] bd &#xA;    --ON bc.date = bd.date &#xA;    --AND bc.customerkey = bd.code&#xA;JOIN stage..vmonth vm &#xA;    ON 1=1&#xA;&#x9;JOIN [Stage].[kharazmi].[budgetproduct] bp ON&#xA;    -- CONCAT(CAST(bc.date AS VARCHAR(4)), vm.mm, '01') = bp.Month AND&#xA;     CONCAT(CAST(bd.date AS VARCHAR(4)), vm.mm, '01') = bp.Month&#xA;&#xA;--WHERE &#xA;--budget IS NOT NULL and &#xA;--bp.month=14040101&#xA;--)a" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
