                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
o.OrderID
,oi.OrderItemID
,o.CustomerRef 
,o.BrokerRef 
,o.AgentRef 
,d.datekey OrderDate
,d1.datekey InvoiceDate
,o.InventoryRef
,o.Number OrderNumber
,o.SalesAreaRef
,o.State
,o.Type
,oi.ProductRef
,i.InvoiceID
, ii.InvoiceItemID
, i.Number InvoiceNumber
,sum(isnull(ii.ReductionAmount,0))ReductionAmount
,sum(isnull(ii.AdditionAmount,0))AdditionAmount
,case when i.OperationalCurrencyExchangeRate &lt;&gt;1 then sum (i.OperationalCurrencyExchangeRate* ii.NetPrice) else sum(ii.NetPrice) end as InvoiceNetPrice
,sum(isnull(ii.Quantity,0))Quantity
--,ivi.ReturnInvoiceItemID 
,ivi.ReturnInvoiceRef
,sum(isnull(ivi.NetPrice,0)) ReturnNetPrice 
,sum(isnull(ivi.Quantity,0)) ReturnQuantity
,sum(isnull(ivi.AdditionAmount,0)) ReturnAdditionAmount
,sum(isnull(ivi.ReductionAmount,0)) ReturnReductionAmount
--into [dw_kharazmi]..Factsales
--select *
  from   kharazmi.[SLS3Order]o 
  left join (  select *   FROM kharazmi.[SLS3OrderItem])oi on o.OrderID=oi.OrderRef
  left join (  select * from kharazmi.RecursiveEntityRelation where  SourceType=1424 and TargetType=1395 ) re on re.SourceRef=oi.OrderItemID 
  left join (  select *   FROM kharazmi.[SLS3InvoiceItem])ii on ii.InvoiceItemID=re.TargetRef
  left join ( select*  FROM kharazmi.[SLS3Invoice])i on i.InvoiceID=ii.InvoiceRef
  left join (  select * from kharazmi.RecursiveEntityRelation where  SourceType=1395 and TargetType=1403 ) re1 on re1.SourceRef=ii.InvoiceItemID
  left join (select *   FROM kharazmi.ReturnInvoiceItem)ivi on ivi.ReturnInvoiceItemID= re1.TargetRef
  left join (select * from kharazmi.[Product])p on p.ProductID= oi.ProductRef
  left join (select * from dimdate) d on d.Gregorian= o.Date 
  left join (select * from dimdate) d1 on d1.Gregorian= i.Date 

  where 
 -- i.InventoryRef in(2, 19,53) --and InvoiceID= 11581
 i.Status in(11,2)
  and (o.isupdate=1 or i.isupdate=1 )
  or (ivi.isinsert=1 or ivi.isupdate=1) 

  group by o.OrderID
,oi.OrderItemID
,o.CustomerRef 
,o.BrokerRef 
,o.AgentRef 
,d.datekey 
,d1.datekey 
,o.InventoryRef
,o.Number 
,o.SalesAreaRef
,o.State
,o.Type
,oi.ProductRef
,i.InvoiceID
, ii.InvoiceItemID
, i.Number 
,i.OperationalCurrencyExchangeRate
--,case when i.OperationalCurrencyExchangeRate &lt;&gt;1 then i.OperationalCurrencyExchangeRate* ii.NetPrice else ii.NetPrice end as InvoiceNetPrice
,ivi.ReturnInvoiceRef</property>
