      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{B497DD00-C1C2-4B58-A27A-FCFA87D02C4B}"
          SQLTask:SqlStatementSource="drop table if exists dw.kharazmi.sku&#xA;GO&#xA;&#xA;select * &#xA;into dw.kharazmi.sku&#xA;from &#xA;(&#xA;SELECT   &#xA;&#x9;po.PartRef, BatchID, b.Number batchnumber, p.Name,p.Code  ,max (LEFT(bc.CharacteristicValue, 10) )AS Expr1, min(LEFT(bc.CharacteristicValue, 10) )Expr2&#xA;&#x9; &#xA;FROM kharazmi.[Batch] b&#xA;join (select * FROM   kharazmi.BatchCharacteristic where CharacteristicRef=12)AS bc on bc.BatchRef=b.BatchID&#xA;join (select * from kharazmi.Part where PartNature=2) p on p.PartID=b.PartRef &#xA;join (select * from kharazmi.Product) po on po.PartRef= p.PartID&#xA;&#xA;where  &#xA;  bc.CharacteristicRef=12&#xA;group by po.PartRef, BatchID, b.Number,p.Code, po.Number,p.Name&#xA;)a &#xA;where &#xA;Expr2 is not null" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
