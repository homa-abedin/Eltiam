        "measures": [
          {
            "name": "MojodiAvalDore",
            "expression": [
              " ",
              "",
              "var fdate= SELECTEDVALUE(DimDate_Big[Jalali_Year]) ",
              "",
              "--VAR fyear=left(fdate,4)",
              "var fidate=CONVERT(CONCATENATE(fdate, \"0101\"), INTEGER)",
              "VAR ProductFilter =",
              "    VALUES(DimProduct[ProductKey])",
              "return  ",
              "CALCULATE(sum(FactInventory[Quantity]) - sum(FactInventory[QtyReserved])",
              ", FactInventory[fiscalyearkey]=fdate",
              ", FactInventory[DateKey]<=fidate",
              ",TREATAS(ProductFilter, FactInventory[ProductKey])",
              ",  REMOVEFILTERS(DimDate_Big)",
              "",
              ")",
              "   "
            ]
          },
          {
            "name": "MojodiAmade",
            "expression": [
              " ",
              "VAR selectedDate = MAX(DimDate_Big[datekey])",
              "VAR fyear= LOOKUPVALUE(",
              "    DimDate_Big[Jalali_Year], ",
              "    DimDate_Big[datekey], ",
              "    selectedDate)",
              "",
              "",
              "",
              "return ",
              "CALCULATE([Mojodi] -[Reserved], FactInventory[fiscalyearkey]=fyear, FactInventory[DateKey]<=selectedDate)"
            ]
          },
          {
            "name": "Mojodi",
            "expression": [
              " ",
              "var   selectedDate = MAX(DimDate_Big[datekey])",
              "VAR fyear=convert (left(selectedDate ,4), INTEGER)",
              "VAR ProductFilter =",
              "    VALUES(DimProduct[ProductKey])",
              "RETURN",
              "",
              "CALCULATE(sum(FactInventory[Quantity])",
              ", FactInventory[fiscalyearkey]=fyear",
              ", FactInventory[DateKey]<=selectedDate",
              ",TREATAS(ProductFilter, FactInventory[ProductKey])",
              ",ALL(DimDate_Big)",
              ")",
              "",
              "",
              ""
            ]
          },
          {
            "name": "Reserved",
            "expression": [
              "",
              "",
              "VAR selectedDate = MAX(DimDate_Big[datekey])",
              "VAR fyear= LOOKUPVALUE(",
              "    DimDate_Big[Jalali_Year], ",
              "    DimDate_Big[datekey], ",
              "    selectedDate)",
              "RETURN",
              "",
              "CALCULATE(sum(FactInventory[QtyReserved]), FactInventory[fiscalyearkey]=fyear, FactInventory[DateKey]<=selectedDate)"
            ]
          },
          {
            "name": "OntheWay",
            "expression": [
              "",
              "VAR selectedDate = MAX(DimDate_Big[datekey])",
              "VAR fyear= LOOKUPVALUE(",
              "    DimDate_Big[Jalali_Year], ",
              "    DimDate_Big[datekey], ",
              "    selectedDate)",
              "RETURN",
              "",
              "CALCULATE(sum(FactInventory[QtyOnTheWay]), FactInventory[fiscalyearkey]=fyear, FactInventory[DateKey]<=selectedDate)"
            ]
          },
          {
            "name": "InventoryTurnoverRate",
            "description": "0",
            "expression": [
              "",
              "",
              "VAR selectedDate = MAX(DimDate_Big[datekey])",
              "VAR fyear= LOOKUPVALUE(",
              "    DimDate_Big[Jalali_Year], ",
              "    DimDate_Big[datekey], ",
              "    selectedDate)",
              "",
              "",
              "Var AVGMojodi=DIVIDE(( [RialMojodi]+  [RialMojodiAvalDore]),2)",
              " var sales= FactSales[NetCost]",
              " var ITR=ABS( DIVIDE(sales, AVGMojodi))",
              " return",
              " DIVIDE([CountWorkDay], ITR)  "
            ],
            "formatString": "#,0"
          },
          {
            "name": "RialMojodiAvalDore",
            "expression": [
              " ",
              "",
              "var fdate= SELECTEDVALUE(DimDate_Big[Jalali_Year]) ",
              "",
              "--VAR fyear=left(fdate,4)",
              "var fidate=CONVERT(CONCATENATE(fdate, \"0101\"), INTEGER)",
              "VAR ProductFilter =",
              "    VALUES(DimProduct[ProductKey])",
              "var    lastcost=CALCULATE(",
              "        LASTNONBLANKVALUE(",
              "            FactInventory[DateKey],",
              "            MIN(FactInventory[Cost])",
              "        ),",
              "        FactInventory[DateKey] <= fidate,",
              "        TREATAS(ProductFilter, FactInventory[ProductKey])",
              ",  REMOVEFILTERS(DimDate_Big)",
              "    )",
              "",
              "",
              "",
              "return  ",
              "DIVIDE(",
              "CALCULATE(",
              "\tsumx(FactInventory, FactInventory[Quantity]* lastcost) -",
              "\t sumx(FactInventory,FactInventory[QtyReserved]*FactInventory[Cost])",
              "\t, FactInventory[fiscalyearkey]=fdate",
              "    , FactInventory[DateKey]<=fidate ",
              "    ,TREATAS(ProductFilter, FactInventory[ProductKey])",
              "    ,ALL(DimDate_Big)",
              ")",
              ",1000000 )"
            ]
          },
          {
            "name": "RialMojodi",
            "expression": [
              " ",
              "var   selectedDate = MAX(DimDate_Big[datekey])",
              "",
              "VAR fyear=left(selectedDate ,4)",
              "VAR ProductFilter =",
              "    VALUES(DimProduct[ProductKey])",
              "var    lastcost=CALCULATE(",
              "        LASTNONBLANKVALUE(",
              "            FactInventory[DateKey],",
              "            MIN(FactInventory[Cost])",
              "        ),",
              "        FactInventory[DateKey] <= selectedDate ,",
              "        TREATAS(ProductFilter, FactInventory[ProductKey])",
              "    )",
              "",
              "RETURN",
              "DIVIDE(",
              "CALCULATE(sumx(FactInventory,FactInventory[Quantity] *lastcost)",
              ", FactInventory[fiscalyearkey]=CONVERT(fyear, INTEGER)",
              ", FactInventory[DateKey]<=selectedDate ",
              ",TREATAS(ProductFilter, FactInventory[ProductKey])",
              ",ALL(DimDate_Big)",
              "),1000000)"
            ],
            "formatString": "#,0"
          },
          {
            "name": "AVGMojodi",
            "expression": "DIVIDE( [RialMojodi]+[RialMojodiAvalDore],2)",
            "formatString": "0.00"
          },
          {
            "name": "productColorInventoryRatio",
            "expression": [
              "",
              " SWITCH(",
              "        TRUE(), ",
              "        --ISINSCOPE(DimProduct[Product]), \"#6c6c6c\",",
              "[InventoryTurnoverRate]<=365 && [Mojodi]<=[NetQuantity]  , \"#52AD58\"",
              ",[InventoryTurnoverRate]<=365 && [Mojodi]>[NetQuantity] ,\"#01870A\"",
              ",[InventoryTurnoverRate]>365 && [Mojodi]<=[NetQuantity], \"#D46C6C\"",
              ",[InventoryTurnoverRate]>365 && [Mojodi]>[NetQuantity], \"#B50303\"",
              ", \"#6c6c6c\"",
              ")",
              "",
              ""
            ]
          },
          {
            "name": "CountOfFastMovingItems",
            "expression": [
              "",
              "",
              "CALCULATE(",
              "    DISTINCTCOUNT(FactInventory[ProductKey]),",
              "       FILTER(",
              "     VALUES(DimProduct[ProductKey]),",
              "         FactInventory[InventoryTurnoverRate]<= 365 ",
              "    )",
              "",
              ")"
            ]
          },
          {
            "name": "CountOfSolwMovingItems",
            "expression": [
              "",
              "CALCULATE(",
              "    DISTINCTCOUNT(dimproduct[ProductKey]),",
              "    FILTER(",
              "        VALUES(DimProduct[ProductKey]),",
              "         FactInventory[InventoryTurnoverRate]> 365 ",
              "    )",
              ")",
              "",
              "",
              ""
            ]
          },
          {
            "name": "CountOfProduct In inventory",
            "expression": " CALCULATE(DISTINCTCOUNT( FactInventory[ProductKey]), all(dimproduct))"
          },
          {
            "name": "RatioSlowMoving",
            "expression": "DIVIDE( [CountOfSolwMovingItems],[CountOfProduct In inventory])",
            "formatString": "0 %;-0 %;0 %"
          },
          {
            "name": "RatioFastMoving",
            "expression": "DIVIDE( [CountOfFastMovingItems],[CountOfProduct In inventory])",
            "formatString": "0 %;-0 %;0 %"
          },
          {
            "name": "RialFastMovingALLItems",
            "expression": [
              "",
              "",
              "CALCULATE(",
              "    [RialMojodi],   ",
              "    FILTER(",
              "        VALUES(DimProduct[ProductKey]),",
              "        [InventoryTurnoverRate]<= 365",
              "    )",
              ")"
            ],
            "formatString": "#,0"
          },
          {
            "name": "RialSlowMovingALLItems",
            "expression": [
              "",
              "",
              "CALCULATE(",
              "    [RialMojodi],   ",
              "    FILTER(",
              "        VALUES(DimProduct[ProductKey]),",
              "        [InventoryTurnoverRate] > 365",
              "    )",
              ")"
            ],
            "formatString": "#,0"
          },
          {
            "name": "RialALLItemsInInventory",
            "expression": [
              "",
              "",
              "CALCULATE(",
              "  DIVIDE(  sumx(FactInventory,FactInventory[Quantity]*FactInventory[Cost]), 1000000), ",
              "    ",
              "      ALL(dimproduct)",
              "   ",
              "",
              ")"
            ]
          },
          {
            "name": "RatioRialAllTOSLow",
            "expression": " DIVIDE([RialSlowMovingALLItems], [RialALLItemsInInventory])",
            "formatString": "0 %;-0 %;0 %"
          },
          {
            "name": "RatioRialAllTOFast",
            "expression": " DIVIDE([RialFastMovingALLItems], [RialALLItemsInInventory])",
            "formatString": "0 %;-0 %;0 %"
          },
          {
            "name": "testmeasure",
            "expression": [
              " ",
              "",
              "",
              "VAR selectedDate = MAX(DimDate_Big[datekey])",
              "VAR fyear = LEFT(selectedDate, 4)",
              "VAR ProductFilter = VALUES(DimProduct[ProductKey])",
              "",
              "VAR centersTable =",
              "    ADDCOLUMNS(",
              "        ALL(dimcenter),",
              "        \"CenterMargin\",",
              "            VAR lastcost =",
              "                CALCULATE(",
              "                    LASTNONBLANKVALUE(",
              "                        FactInventory[DateKey],",
              "                        MIN(FactInventory[Cost])",
              "                    ),",
              "                    FactInventory[DateKey] <= selectedDate,",
              "                    TREATAS(ProductFilter, FactInventory[ProductKey]),",
              "                    TREATAS(VALUES(dimcenter[CenterKey]), FactInventory[CenterId])",
              "                )",
              "            RETURN",
              "                DIVIDE(",
              "                    CALCULATE(",
              "                        SUMX(",
              "                            FactInventory,",
              "                            FactInventory[Quantity] * lastcost",
              "                        ),",
              "                        FactInventory[fiscalyearkey] = CONVERT(fyear, INTEGER),",
              "                        FactInventory[DateKey] <= selectedDate,",
              "                        TREATAS(ProductFilter, FactInventory[ProductKey]),",
              "                        TREATAS(VALUES(dimcenter[CenterKey]), FactInventory[Centerid]),",
              "                        ALL(DimDate_Big)",
              "                    ),",
              "                    1000000",
              "                )",
              "    )",
              "",
              "VAR totalMargin = SUMX(centersTable, [CenterMargin])",
              "VAR centerCount = COUNTROWS(centersTable)",
              "",
              "RETURN DIVIDE(totalMargin, centerCount)",
              ""
            ]
          },
          {
            "name": "ColorInventory",
            "expression": [
              "",
              "VAR cntcenter = CALCULATE(DISTINCTCOUNT(dimcenter[CenterKey]), ALL(dimcenter))",
              "VAR _avgDisc = DIVIDE(CALCULATE(FactInventory[InventoryTurnoverRate], ALL(dimcenter)), cntcenter)",
              "VAR _avgMargin = DIVIDE(CALCULATE([RialMojodi], ALL(dimcenter)), cntcenter)",
              "RETURN",
              "",
              "SWITCH(",
              "    TRUE(),",
              "    ",
              "    // اگر فقط در سطح مرکز هستیم",
              "    ISINSCOPE(dimcenter[CenterName]) && NOT(ISINSCOPE(dimproduct[Product])),\"888888\"",
              "    /*SWITCH(TRUE(),",
              "        FactInventory[InventoryTurnoverRate] >= _avgDisc && [RialMojodi] >= _avgMargin , \"#B50303\",",
              "        FactInventory[InventoryTurnoverRate] >  _avgDisc && [RialMojodi] <  _avgMargin , \"#D46C6C\",",
              "        FactInventory[InventoryTurnoverRate] <=  _avgDisc && [RialMojodi] >= _avgMargin , \"#01870A\",",
              "        FactInventory[InventoryTurnoverRate] <  _avgDisc && [RialMojodi] <  _avgMargin , \"#52AD58\",",
              "        \"Black\"",
              "    )*/",
              ",",
              "",
              "    // اگر در سطح محصول هستیم (یا پایین‌تر)",
              "    ISINSCOPE(dimproduct[Product]),",
              "    [productColorInventoryRatio],",
              "",
              "    // حالت پیش‌فرض",
              "    BLANK()",
              ")",
              ""
            ]
          }
        ]
      },
