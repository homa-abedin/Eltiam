        ],
        "partitions": [
          {
            "name": "Partition",
            "dataView": "full",
            "source": {
              "type": "m",
              "expression": [
                "let",
                "    Source = #\"SQL/192 168 10 10;DW\",",
                "    dbo_FactFinance = Source{[Schema=\"dbo\",Item=\"FactFinance\"]}[Data]",
                "in",
                "    dbo_FactFinance"
              ]
            }
          }
        ],
        "measures": [
          {
            "name": "Balanced",
            "description": "0",
            "expression": [
              " DIVIDE(CALCULATE (",
              "    SUM ( FactFinance[DebitBalance] ),",
              "    NOT ( FactFinance[FINDocHeader_FINDocType] IN { 2, 4 } )",
              "),1000000)"
            ],
            "formatString": "#,0"
          },
          {
            "name": "SalesFinance",
            "expression": [
              "if (ISBLANK( abs(CALCULATE([Balanced], DimAccountL1_3[IdL2]= 28))) ",
              ", 0 , abs(CALCULATE([Balanced], DimAccountL1_3[IdL2]= 28)) )",
              "",
              ""
            ],
            "formatString": "#,0"
          },
          {
            "name": "Expenses",
            "description": "0",
            "expression": [
              "  CALCULATE([Balanced], DimAccountL1_3[IdL2] in {31,34,35}) *-1",
              "",
              ""
            ],
            "formatString": "#,0"
          },
          {
            "name": "OtherIncome",
            "description": "0",
            "expression": [
              "  abs( CALCULATE([Balanced], DimAccountL1_3[IdL2] = 29) )",
              ""
            ],
            "formatString": "#,0"
          },
          {
            "name": "OtherExpenses",
            "expression": [
              "IF (",
              "    ISBLANK ( CALCULATE ( [Balanced], DimAccountL1_3[IdL2] IN { 32 } ) ),",
              "    0,",
              "    CALCULATE ( [Balanced], DimAccountL1_3[IdL2] IN { 32 } ) * -1",
              ")",
              ""
            ]
          },
          {
            "name": "FinancialExpenses",
            "expression": [
              "  CALCULATE([Balanced], DimAccountL1_3[IdL2] in {33}) *-1",
              "",
              "",
              ""
            ]
          },
          {
            "name": "OtherNONOperatingIncome",
            "expression": [
              "abs( CALCULATE([Balanced], DimAccountL1_3[IdL2] in {30}))",
              ""
            ]
          },
          {
            "name": "IncomeTaxExpense",
            "description": "0",
            "expression": "if ([ProfitOperationsBeforeTax] <0 , 0, ([ProfitOperationsBeforeTax]*0.25)*-1)",
            "formatString": "#,0"
          },
          {
            "name": "GrossProfit",
            "expression": " [SalesFinance]+ ([CoGS])",
            "formatString": "#,0"
          },
          {
            "name": "OperatingProfit",
            "expression": " [GrossProfit]+[Expenses]+[OtherIncome]+[OtherExpenses]",
            "formatString": "#,0"
          },
          {
            "name": "ProfitOperationsBeforeTax",
            "expression": [
              " ",
              "[OperatingProfit]+",
              "FactFinance[FinancialExpenses]+",
              "[OtherNONOperatingIncome]"
            ],
            "formatString": "#,0"
          },
          {
            "name": "NetProfit",
            "expression": " [ProfitOperationsBeforeTax]+[IncomeTaxExpense]",
            "formatString": "#,0"
          },
          {
            "name": "Cash",
            "expression": [
              " CALCULATE([Balanced], FILTER (",
              "        FactFinance,",
              "        FactFinance[IDL2] = 14",
              "            && FactFinance[IDL3] <> 423",
              "    ))"
            ],
            "formatString": "#,0"
          },
          {
            "name": "CoGS",
            "expression": [
              " CALCULATE([Balanced]*-1, ",
              "        ",
              "        FactFinance[IDL2] in { 37,36}      ",
              "     ",
              "    )"
            ],
            "formatString": "#,0"
          },
          {
            "name": "ProfitMargin",
            "expression": "DIVIDE( [GrossProfit], [SalesFinance]) ",
            "formatString": "0.0 %;-0.0 %;0.0 %"
          },
          {
            "name": "NetProfitMargin",
            "expression": "DIVIDE( [NetProfit], [SalesFinance]) ",
            "formatString": "0.0 %;-0.0 %;0.0 %"
          },
          {
            "name": "operatingMargin",
            "expression": "DIVIDE( [OperatingProfit], [SalesFinance]) ",
            "formatString": "0.0 %;-0.0 %;0.0 %"
          },
          {
            "name": "ExpenseslastYear",
            "expression": [
              "",
              "CALCULATE (",
              "    ABS ( FactFinance[Expenses]  ),",
              "    USERELATIONSHIP ( FactFinance[DateKey], DimDate_Big[LastYearDateKey] )",
              "    ",
              ")",
              "  /*  CALCULATE(ABS(FactFinance[Expenses]),FILTER(ALL ( DimDate_Big[Jalali_Year] )",
              ", DimDate_Big[Jalali_Year]=max( DimDate_Big[Jalali_Year])-1",
              ")",
              " )*/"
            ]
          },
          {
            "name": "SalesFinancelastYear",
            "expression": [
              "",
              " ",
              "    CALCULATE(ABS(FactFinance[SalesFinance]),",
              " USERELATIONSHIP ( FactFinance[DateKey], DimDate_Big[LastYearDateKey] )",
              ")"
            ]
          },
          {
            "name": "cashlastYear",
            "expression": [
              "",
              " ",
              "    CALCULATE(FactFinance[Cash], ",
              "USERELATIONSHIP ( FactFinance[DateKey], DimDate_Big[LastYearDateKey] )",
              ")",
              " "
            ]
          },
          {
            "name": "COGSlastYear",
            "expression": [
              "",
              " ",
              "    CALCULATE(ABS(FactFinance[CoGSmosbat]), ",
              "USERELATIONSHIP ( FactFinance[DateKey], DimDate_Big[LastYearDateKey] )",
              ")",
              " ",
              ""
            ]
          },
          {
            "name": "NetprofitlastYear",
            "expression": [
              "",
              " ",
              "    CALCULATE(ABS(FactFinance[NetProfit]),",
              " USERELATIONSHIP ( FactFinance[DateKey], DimDate_Big[LastYearDateKey] )",
              " )"
            ]
          },
          {
            "name": "RatioIncome",
            "expression": "DIVIDE(( [SalesFinance]- [SalesFinancelastYear] ), [SalesFinancelastYear])",
            "formatString": "0 %;-0 %;0 %"
          },
          {
            "name": "RatioExpenses",
            "expression": "DIVIDE( ([ExpensesMosbat]- [ExpenseslastYear]), [ExpenseslastYear])",
            "formatString": "0 %;-0 %;0 %"
          },
          {
            "name": "RatioCash",
            "expression": "DIVIDE(( [Cash]-[cashlastYear]),[cashlastYear])",
            "formatString": "0 %;-0 %;0 %"
          },
          {
            "name": "CoGSRatio",
            "expression": [
              "DIVIDE(( [CoGSmosbat]- [COGSlastYear]), [COGSlastYear])",
              ""
            ],
            "formatString": "0 %;-0 %;0 %"
          },
          {
            "name": "NetProfitRatio",
            "expression": [
              " DIVIDE(([CoGSmosbat]-[COGSlastYear]), [COGSlastYear])",
              "",
              ""
            ],
            "formatString": "0 %;-0 %;0 %"
          },
          {
            "name": "ExpensesMosbat",
            "expression": "ABS( [Expenses])",
            "formatString": "#,0"
          },
          {
            "name": "CoGSmosbat",
            "expression": [
              " ABS([CoGS])",
              ""
            ]
          },
          {
            "name": "CashMosbat",
            "expression": " ABS([Cash])"
          },
          {
            "name": "ProfitMargin_1",
            "expression": " 1-[ProfitMargin]",
            "formatString": "0 %;-0 %;0 %"
          },
          {
            "name": "NetProfitMargin_1",
            "expression": " 1 - [NetProfitMargin]",
            "formatString": "0 %;-0 %;0 %"
          },
