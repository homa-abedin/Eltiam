          {
            "name": "HesabDaryaftani",
            "description": "0",
            "expression": [
              "CALCULATE([Balanced], DimAccountL1_3[IdL2] in {15,16,56})",
              ""
            ],
            "formatString": "#,0"
          },
          {
            "name": "DSO",
            "expression": " DIVIDE([HesabDaryaftani], FactFinance[SalesFinance])*365",
            "formatString": "#,0"
          },
          {
            "name": "Inventory beginning of year",
            "expression": [
              " ",
              "DIVIDE(",
              "    CALCULATE(",
              "        SUM(FactFinance[DebitBalance]),",
              "        (FactFinance[FINDocHeader_FINDocType] = 3 || FactFinance[FINDocHeader_FINDocType] = 5),",
              "        DimAccountL1_3[IdL2] = 17",
              "    ),",
              "    1000000",
              ")"
            ],
            "formatString": "#,0"
          },
          {
            "name": "Inventory End of year",
            "expression": [
              " ",
              "",
              "CALCULATE([Balanced], DimAccountL1_3[IdL2] = 17)"
            ],
            "formatString": "#,0"
          },
          {
            "name": "DIO",
            "expression": "DIVIDE( ([Inventory beginning of year]+[Inventory End of year]), [CoGSmosbat])* 365",
            "formatString": "#,0"
          },
          {
            "name": "HesabPardakhtani",
            "expression": [
              " CALCULATE([Balanced], FILTER(",
              "        DimAccountL1_3,",
              "        (",
              "            DimAccountL1_3[IdL3] = 45 ",
              "            || DimAccountL1_3[IdL3] = 7",
              "            || DimAccountL1_3[IdL2] = 1",
              "            || DimAccountL1_3[IdL2] = 2",
              "            || DimAccountL1_3[IdL2] = 23",
              "        )",
              "        && NOT(DimAccountL1_3[IdL3] = 95)",
              "    ))"
            ],
            "formatString": "#,0"
          },
          {
            "name": "DPO",
            "expression": [
              "  ",
              "CALCULATE(",
              "    DIVIDE(",
              "        ABS([HesabPardakhtani]),",
              "        [CoGSmosbat]",
              "    )",
              "    *[SelectedDaysPerYear] , all(DimDate_Big[datekey]))"
            ],
            "formatString": "#,0"
          },
          {
            "name": "CCC",
            "expression": "[DIO] + [DSO] - [DPO]",
            "formatString": "#,0"
          },
          {
            "name": "PishPardakht",
            "expression": [
              " CALCULATE([Balanced], ",
              "DimAccountL1_3[IdL3] in {35  ,38,39,49,273,319,366,437})",
              ""
            ],
            "formatString": "#,0"
          },
          {
            "name": "DaraiiJari",
            "expression": " [PishPardakht]+ [Inventory End of year] +[HesabDaryaftani]+[Cash]",
            "formatString": "#,0"
          },
          {
            "name": "MaliyatPardakhtani",
            "expression": " CALCULATE([Balanced], DimAccountL1_3[IdL3]= 95)",
            "formatString": "#,0"
          },
          {
            "name": "TashilatMali",
            "expression": " CALCULATE([Balanced], DimAccountL1_3[IdL3] in {439,440,441,423})",
            "formatString": "#,0"
          },
          {
            "name": "BedehiJari",
            "expression": [
