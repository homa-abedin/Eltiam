      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{F941566B-4675-4147-9490-01E6DB4EE689}"
          SQLTask:SqlStatementSource="WITH ProductCounts AS (&#xA;    SELECT &#xA;        GTIN,&#xA;        ProductCode,&#xA;        COUNT(*) AS cnt&#xA;    FROM Stage.Web.Barij_Inventory&#xA;    GROUP BY GTIN, ProductCode&#xA;),&#xA;GTINMode AS (&#xA;    SELECT *&#xA;    FROM (&#xA;        SELECT &#xA;            GTIN,&#xA;            ProductCode,&#xA;            cnt,&#xA;            ROW_NUMBER() OVER (PARTITION BY GTIN ORDER BY cnt DESC, ProductCode) AS rn&#xA;        FROM ProductCounts&#xA;    ) ranked&#xA;    WHERE rn = 1&#xA;),&#xA;DuplicateGTINs AS (&#xA;    SELECT GTIN&#xA;    FROM ProductCounts&#xA;    GROUP BY GTIN&#xA;    HAVING COUNT(*) &gt; 1&#xA;)&#xA;UPDATE s&#xA;SET ProductCode = g.ProductCode&#xA;FROM Stage.Web.Barij_Inventory s&#xA;JOIN GTINMode g&#xA;    ON s.GTIN = g.GTIN&#xA;JOIN DuplicateGTINs d&#xA;    ON s.GTIN = d.GTIN&#xA;WHERE s.ProductCode &lt;&gt; g.ProductCode;&#xA;&#xA;--SELECT &#xA;--    s.GTIN,&#xA;--    s.ProductCode AS OldProductCode,&#xA;--    g.ProductCode AS NewProductCode,&#xA;--    COUNT(*) AS AffectedCount&#xA;--FROM Stage.Web.Barij_Inventory s&#xA;--JOIN GTINMode g ON s.GTIN = g.GTIN&#xA;--JOIN DuplicateGTINs d ON s.GTIN = d.GTIN&#xA;--WHERE s.ProductCode &lt;&gt; g.ProductCode&#xA;--GROUP BY s.GTIN, s.ProductCode, g.ProductCode;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
