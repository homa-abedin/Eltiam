            "Provider=Microsoft.ACE.OLEDB.12.0;" +
            $"Data Source={Variables.filname3};" +
            "Extended Properties=\"Excel 12.0 Xml;HDR=YES;IMEX=1\";";

        bool fireAgain = false;

        using (OleDbConnection conn = new OleDbConnection(connStr))
        {
            try
            {
                conn.Open();
                ComponentMetaData.FireInformation(0, "CustomLog", "Connection opened to Excel file.", "", 0, ref fireAgain);

                // همه شیت رو می‌خونه
                OleDbDataAdapter da = new OleDbDataAdapter("SELECT * FROM [Sheet1$]", conn);
                DataTable dt = new DataTable();
                da.Fill(dt);

                int insertedRows = 0;

                // از سطر سوم (index=2) تا یکی مونده به آخر
                for (int i = 2; i < dt.Rows.Count - 1; i++)
                {
                    DataRow row = dt.Rows[i];

                    Output0Buffer.AddRow();
                    Output0Buffer.ProductCode = row[0]?.ToString();
                    Output0Buffer.ProductName = row[1]?.ToString();
                    Output0Buffer.Shape = row[2]?.ToString();
                    Output0Buffer.Unit = row[3]?.ToString();
                    Output0Buffer.Branch = row[4]?.ToString();
                    Output0Buffer.QuantitySold = row[5] == DBNull.Value ? 0 : Convert.ToInt32(row[5]);
                    Output0Buffer.SalesAmount = row[6] == DBNull.Value ? 0 : Convert.ToDecimal(row[6]);
                    Output0Buffer.ReturnQty = row[7] == DBNull.Value ? 0 : Convert.ToInt32(row[7]);
                    Output0Buffer.ReturnAmount = row[8] == DBNull.Value ? 0 : Convert.ToDecimal(row[8]);
                    Output0Buffer.InTransitQuantity = row[9] == DBNull.Value ? 0 : Convert.ToInt32(row[9]);
                    Output0Buffer.InTransitAmount = row[10] == DBNull.Value ? 0 : Convert.ToDecimal(row[10]);
                    Output0Buffer.InventoryQuantity = row[11] == DBNull.Value ? 0 : Convert.ToInt32(row[11]);
                    Output0Buffer.InventoryAmount = row[12] == DBNull.Value ? 0 : Convert.ToDecimal(row[12]);

                    // تاریخ از اسم فایل
                    Output0Buffer.Date = int.Parse(System.IO.Path.GetFileNameWithoutExtension(Variables.filname3));
                    Output0Buffer.IsInsert = 1;
                    Output0Buffer.CenterCode = 0;

                    insertedRows++;
                }

                ComponentMetaData.FireInformation(0, "CustomLog", $"Rows inserted: {insertedRows}", "", 0, ref fireAgain);
            }
            catch (Exception ex)
            {
                processResult = "Fail"; // فقط مقدار local تغییر می‌کنه
                ComponentMetaData.FireInformation(0, "CustomLog", $"Exception: {ex.Message}", "", 0, ref fireAgain);
            }
        }
    }


    /*
      Add rows by calling the AddRow method on the member variable named "<Output Name>Buffer".
      For example, call MyOutputBuffer.AddRow() if your output was named "MyOutput".
    */
}

