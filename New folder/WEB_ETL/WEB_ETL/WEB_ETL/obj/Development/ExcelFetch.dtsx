                Name="ScriptMain.cs"
                Encoding="UTF8"><![CDATA[#region Namespaces
using System;
using System.Data;
using Microsoft.SqlServer.Dts.Runtime;
using System.Collections;
#endregion

[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
{
    public void Main()
    {
        bool skip = false;

        try
        {
            // 1️⃣ خواندن نام فایل جاری
            string currentFile = Dts.Variables["User::filname2"].Value.ToString();

            // 2️⃣ خواندن لیست فایل‌های پردازش‌شده
            object obj = Dts.Variables["User::ProcessedFilesList1"].Value;

            if (obj != null)
            {
                // تبدیل Object Variable به DataTable
                DataTable dt = obj as DataTable;

                if (dt != null)
                {
                    foreach (DataRow row in dt.Rows)
                    {
                        if (row[0].ToString() == currentFile)
                        {
                            skip = true;
                            break;
                        }
                    }
                }
            }

            // 3️⃣ ست کردن متغیر SkipFile1
            Dts.Variables["User::SkipFile1"].Value = skip;

            Dts.TaskResult = (int)ScriptResults.Success;
        }
        catch (Exception ex)
        {
            // خطا رو گزارش بده و Task رو Fail کن
            Dts.Events.FireError(0, "Script Task", ex.Message, "", 0);
            Dts.TaskResult = (int)ScriptResults.Failure;
        }
    }

    enum ScriptResults
    {
        Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
        Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
    };
}]]></ProjectItem>
