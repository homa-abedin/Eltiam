select 
Date
, ShomarehFaktor
,2 as type
,0  as isprize
,convert ( int, ReturnQty ) Quantity
,cast ( ReturnAmountprice  as decimal (18,2))  as Returnprice 
, dc.id as centerid
,case when isnull(cu.gln ,0)=0 then 48000 else  cu.gln end as gln
,p.kharazmi productcode 
,N'Momtaz' as pakhsh, 6 as valuepakhsh  
,sku.id as batchid
from stage.web.Momtaz_CustomerSales s
JOIN (select * from dw.kharazmi.DimCenterPakhsh) dc ON dc.momtaz = s.CenterCode
 join (select * from web.mapproduct) p on p.momtaz= s.ProductCode
 join (select * from stage.web.Momtaz_Customer)cu on cu.customerCode=s.CustomerCode
  join (select * from dw.kharazmi.dimsku)sku on sku.productcode=  p.kharazmi and sku.batchno= '-------'
where invOperationCode in (102) and ReturnAmountDiscount =0
and isinsert=1

union all
select 
Date
, ShomarehFaktor 
,2 as type
,1  as isprize

, cast (ReturnAmountDiscount/Price  as int)  Quantity
 , cast ( ReturnAmountDiscount  as decimal (18,2))  as Returnprice 
--,AmountDiscount
