     s.Quantity  ,
      Price  ,
      0 isprize,
     
    dc.Id as centerid,
    s.GLN,
	p.kharazmi productcode
    --COALESCE(c.GTIN, v.Valid_GTIN) AS GTIN -- پر کردن NULLها,
	,N'razi' as pakhsh, 24000 as  [valuepakhsh]
	,sku.id batchid
	--select * 
