--select *
from stage.web.Momtaz_ReturnSalesNew s
join Stage.web.Momtaz_Products p on p.productKey=s.productKey
join (select * from web.mapproduct) p1 on p1.momtaz= p.ProductCode

join (select * from Stage.web.Momtaz_SKUs) sku on sku.skuKey= s.skuKey
join Stage.web.Momtaz_Customer cu on cu.customerKey=s.customerKey
join dw.kharazmi.DimCenterPakhsh  cp on cp.momtaz1404 = s.centerKey
join (select * from dw.kharazmi.dimsku)sku1 on sku1.productcode=  p1.kharazmi and sku1.batchno= sku.batchNumber

where isinsert=1</property>
