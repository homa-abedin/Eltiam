                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @LastDate INT;
SELECT @LastDate = ISNULL(MAX(datekey), 14030101)
FROM stage.kharazmi.factreturnsaleseltiam;


with statuse as (
select distinct SalesOrderHeaderKey ,SalesOrderStatusKey from  eltiam.SLSSalesOrderStatusSet where   SalesOrderStatusKey=5
)
, pro as (
select  MIN(kharazmi)kharazmi, eltiam from  stage.web.mapproduct where eltiam is not null group by eltiam
)
, dimpro  as (

select * from dw..dimproduct where SupplierCode=100
)
, sales as (
select  
	    h.ReturnNumber,IsDiscountSKU,
		h.id,
	    h.IsApproved , dp.ProductCode,h.CustomerKey,ApprovedDateKey,
	   Ds.Quantity ,D.Price , SKUKey, h.CenterKey
	   --select *
from  eltiam.SLSSalesReturnDetails D
LEFT JOIN eltiam.SLSSalesReturnHeaders H
	ON H.Id = D.SalesReturnHeaderKey
join (select * from dimpro) dp on dp.ProductKey= d.ProductKey
join (select * from eltiam.SLSSalesReturnDetailSKUs)ds on  ds.SalesReturnDetailKey= d.id

	where
ApprovedDateKey&gt;=   @LastDate
and h.id is not null and 
H.ReturnNumber  is not null   


)
--select * from sales




SELECT  
	p.kharazmi productcode,
	ApprovedDateKey AS DateKey,
	p1.Id as centerID,
	c.GLN ,
	ReturnNumber InvoiceNumber,
	case when  IsApproved=1 then Quantity else 0 end as Quantity,
	case when  IsApproved=1 then  Quantity * Price  else 0 end as amount,
	0 as isprize,
	sku1.id batchid 
	,N'eltiam' as pakhsh
	, 20000 as  [valuepakhsh]
	, 1 as isinsert
	--into dw..factsales
	--select *
from  sales s
join (select * from eltiam.WHSSKUs)sku on sku.Id= s.SKUKey 
join (select * from statuse  )  s1 on s1.SalesOrderHeaderKey=s.Id 
join (select * from dw.kharazmi.dimcenterpakhsh   where eltiam is not null ) p1 on p1.eltiam= s.CenterKey
join (select * from  pro) p on p.eltiam= s.ProductCode
join (select * from dw..dimcustomer  ) c on c.customerkey= s.customerkey
join (select * from dw.kharazmi.dimsku) sku1 on sku1.batchno=cast ( sku.BatchNumber as varchar(10) ) and sku1.productcode= p.kharazmi
where IsDiscountSKU=0


 union all
SELECT  
	p.kharazmi productcode,
	ApprovedDateKey AS DateKey,
	p1.Id as centerID,
	c.GLN ,
	ReturnNumber InvoiceNumber,
	case when  IsApproved=1 then Quantity else 0 end as Quantity,
	case when  IsApproved=1 then  Quantity * Price  else 0 end as amount,
	1 as isprize,
	sku1.id batchid 
	,N'eltiam' as pakhsh
	, 20000 as  [valuepakhsh]
	, 1 as isinsert
	--into dw..factsales
	--select *
from  sales s
join (select * from eltiam.WHSSKUs)sku on sku.Id= s.SKUKey 
join (select * from statuse  )  s1 on s1.SalesOrderHeaderKey=s.Id 
join (select * from dw.kharazmi.dimcenterpakhsh   where eltiam is not null ) p1 on p1.eltiam= s.CenterKey
join (select * from  pro) p on p.eltiam= s.ProductCode
join (select * from dw..dimcustomer  ) c on c.customerkey= s.customerkey
join (select * from dw.kharazmi.dimsku) sku1 on sku1.batchno=cast ( sku.BatchNumber as varchar(10) ) and sku1.productcode= p.kharazmi
where IsDiscountSKU=1</property>
