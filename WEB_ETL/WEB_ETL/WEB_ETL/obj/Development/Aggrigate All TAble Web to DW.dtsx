                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
	1 as type,
	s.BillShamsiDate
	,s.ShomarehFaktor  as SalesID,
	abs(s.Quantity)Quantity  ,
	cast( abs( s.Quantity) *Price as decimal(18,2)) as  price ,
	0 isprize,
      sku.id batchID,
	dc.Id as centerid,
	isnull (s.GLN, 12000)GLN,
	p.kharazmi productcode
	--COALESCE(c.GTIN, v.Valid_GTIN) AS GTIN -- پر کردن NULLها,
	,N'alborz' as pakhsh, 12000 as  [valuepakhsh]
	--select * 
FROM web.Alborz_CustomerSales  s
JOIN (select * from dw.kharazmi.DimCenterPakhsh) dc ON dc.Alborz = s.CenterCode
--LEFT JOIN ValidGTINs v ON v.ProductCode = s.ProductCode
 join (select distinct kharazmi, alborz from web.mapproduct) p on p.alborz= s.ProductCode
join (select * from dw.kharazmi.dimsku)sku on sku.productcode=  p.kharazmi and sku.batchno=isnull( s.BatchNo, '')

WHERE SaleType=2
and IsInsert=1</property>
