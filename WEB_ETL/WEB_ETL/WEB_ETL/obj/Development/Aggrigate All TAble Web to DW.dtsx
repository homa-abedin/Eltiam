SELECT 
1 as type,
s.Date
    ,s.ShomarehFaktor  as SalesID,
     s.Quantity  as Quantity,
      s.price  as priceall,
      0 as isprize,
     sku.id batchid,
    dc.Id as centerid,
    s.GLN,
    p.kharazmi productcode -- پر کردن NULLها
	,N'Barij' as pakhsh, 69319 as  [valuepakhsh]
	
	--select * 
FROM web.Barij_CustomerSales  s
JOIN (select * from dw.kharazmi.DimCenterPakhsh) dc ON dc.Barij = s.CenterCode
 join (select  distinct kharazmi, barij from web.mapproduct) p on p.barij= s.ProductCode
--join price pr on pr.ProductCode= p.barij and pr.ShomarehFaktor= s.ShomarehFaktor and pr.Date= s.Date  and s.ProductCode=pr.ProductCode
 join (select * from dw.kharazmi.dimsku)sku on sku.productcode=  p.kharazmi and sku.batchno= ''
WHERE Quantity&lt;&gt;0
and IsInsert=1

union all

SELECT 
	1 as type,
	s.Date
	,s.ShomarehFaktor  as SalesID,
	s.Bonus  as Quantity,
	s.price  as priceall,
	1 as isprize,
	sku.id batchid,
	dc.Id as centerid,
	s.GLN,
	p.kharazmi productcode -- پر کردن NULLها
	,N'Barij' as pakhsh, 69319 as  [valuepakhsh]
	
	--select * 
FROM web.Barij_CustomerSales  s
JOIN (select * from dw.kharazmi.DimCenterPakhsh) dc ON dc.Barij = s.CenterCode
 join (select * from web.mapproduct) p on p.barij= s.ProductCode
--join price pr on pr.ProductCode= p.barij and pr.ShomarehFaktor= s.ShomarehFaktor and pr.Date= s.Date  and s.ProductCode=pr.ProductCode
 join (select * from dw.kharazmi.dimsku)sku on sku.productcode=  p.kharazmi and sku.batchno= ''
WHERE Bonus&lt;&gt;0
and IsInsert=1</property>
