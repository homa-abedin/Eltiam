                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">delete from web.Toba_CustomerSales
where ProductName is null

 ;with price as (
select convert (decimal (18,0) ,sum(Price/Quantity))price , ProductId, FactorID, FactorDate from web.Toba_CustomerSales
where IsPrize=0
group by ProductId, FactorID, FactorDate
)



SELECT 
1 as type,
s.FactorId
    ,s.Factordate  ,
     s.Quantity  ,
      Price  ,
       isprize,
      sku.id batchID,
    dc.Id as centerid,
    s.GLN,
	p.kharazmi productcode
    --COALESCE(c.GTIN, v.Valid_GTIN) AS GTIN -- پر کردن NULLها,
	,N'toba' as pakhsh, 69254 as  [valuepakhsh]
	--select * 
FROM web.Toba_CustomerSales  s
JOIN (select * from dw.kharazmi.DimCenterPakhsh) dc ON dc.toba =  '1'+RIGHT('000' + CAST( CenterId AS VARCHAR(3)), 3) +'0000' 
--LEFT JOIN ValidGTINs v ON v.ProductCode = s.ProductCode
LEFT join (select  distinct kharazmi, toba  from web.mapproduct) p on p.toba= s.ProductId
join (select * from dw.kharazmi.dimsku)sku on sku.productcode=  p.kharazmi and sku.batchno= s.BatchNumber

WHERE RecordTypeId=101 and IsPrize= 0
and IsInsert=1
 union all
SELECT 
1 as type,
s.FactorId
    ,s.Factordate  ,
     s.Quantity  ,
     Quantity*pr.price  as Price  ,
      1 as isprize,
      sku.id batchID,
    dc.Id as centerid,
    s.GLN,
	p.kharazmi productcode
    --COALESCE(c.GTIN, v.Valid_GTIN) AS GTIN -- پر کردن NULLها,
	,N'toba' as pakhsh, 69254 as  [valuepakhsh]
	--select * 
FROM web.Toba_CustomerSales  s
JOIN (select * from dw.kharazmi.DimCenterPakhsh) dc ON dc.toba =  '1'+RIGHT('000' + CAST( CenterId AS VARCHAR(3)), 3) +'0000' 
--LEFT JOIN ValidGTINs v ON v.ProductCode = s.ProductCode
 join (select * from web.mapproduct) p on p.toba= s.ProductId
join price pr on pr.ProductId= p.toba and pr.FactorID= s.FactorID and pr.FactorDate= s.FactorDate
join (select * from dw.kharazmi.dimsku)sku on sku.productcode=  p.kharazmi and sku.batchno= s.BatchNumber


WHERE RecordTypeId=101 and IsPrize= -1
and IsInsert=1</property>
