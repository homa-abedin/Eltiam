                smtp.EnableSsl = true;
                smtp.DeliveryMethod = SmtpDeliveryMethod.Network;
                smtp.UseDefaultCredentials = false; // مهم: این را false قرار دهید
                smtp.Credentials = new System.Net.NetworkCredential("homa.abedinzadeh@gmail.com", "kvzr ugcy pdjl cmjf");
                smtp.Timeout = 20000; // 10 ثانیه تایم‌اوت

                Dts.Events.FireInformation(0, "Send Email", "Connecting to SMTP...", string.Empty, 0, ref fireAgain);

