                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- استفاده از GTIN معتبر برای پر کردن Nullها
SELECT 
2 as type,
s.SalesDate as Date
    ,s.SalesID,
    s.ProdQtyRet ReturnQuantity,
    abs(s.ProdPriceKhales) AS ReturnPrice ,
   0 as  IsPrize,
      sku.id batchID,
    dc.Id as centerid,
    cast ( s.custGln as bigint)GLN,
	p.kharazmi productcode
	,N'Hasti' as pakhsh, 42000 as  [valuepakhsh]
    --s.ProductId
	--select * 
FROM web.hasti_CustomerSales  s
JOIN dw.kharazmi.DimCenterPakhsh dc ON dc.Hasti = s.DSID
join (select distinct kharazmi, hasti from web.mapproduct) p on p.hasti= s.ProdID
join (select * from dw.kharazmi.dimsku)sku on sku.productcode=  p.kharazmi and sku.batchno= s.BatchNo

WHERE s.ProdQtyRet &lt;&gt;0
and IsInsert=1</property>
