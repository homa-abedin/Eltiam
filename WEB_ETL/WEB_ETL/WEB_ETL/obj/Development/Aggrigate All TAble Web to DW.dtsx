                message.To.Add("shomali.kharazmi@gmail.com");
                 message.CC.Add("daarou@gmail.com");
                message.CC.Add("homa.abedinzadeh@gmail.com");

                message.Subject = "محصولات مپ نشده *BI* ";
                message.Body = "با سلام و احترام\r\n به پیوست فایل داروهایی که در فایل کالا مپ نشده اند ارسال شده است. خواهشمند است در اسرع وقت این اطلاعات اصلاح شود.\r\n\r\n\r\n\r\n با تشکر \r\nواحد هوش تجاری گروه خوارزمی\r\n";
