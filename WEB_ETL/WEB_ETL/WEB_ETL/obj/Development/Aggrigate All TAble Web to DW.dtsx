                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH price AS (
    SELECT ProductCode, ShomarehFaktor,CenterCode ,NoeBarge,cast (sum(NetPrice) / sum(Quantity) as  int)price 
    FROM web.Mahya_CustomerSales
    where NetPrice&lt;&gt;0
    GROUP BY  ProductCode, ShomarehFaktor,CenterCode ,NoeBarge
)

SELECT 
1 as type,
s.ShomarehFaktor,
     s.Date Factordate,
    s.Quantity,
    s.NetPrice  ,
     0 IsPrize,
     
    dc.Id as centerid,
    s.Gln,
    p.kharazmi productcode -- پر کردن NULLها
	,N'mahya' as pakhsh, 47000 as  [valuepakhsh]
		, sku.id batchid

	--select *
FROM web.Mahya_CustomerSales s
JOIN dw.kharazmi.DimCenterPakhsh dc ON dc.mahya = s.CenterCode
join (select * from web.mapproduct) p on p.mahya= s.ProductCode
join (select * from dw.kharazmi.dimsku)sku on sku.productcode=  p.kharazmi and sku.batchno= ''

WHERE s.NoeBarge = N'فروش'
 and IsInsert=1 
union all
SELECT 
	1 as type,
	s.ShomarehFaktor,
	s.Date Factordate,
	Bonus as quantity ,
	Bonus*p1.price as netPrice  ,
	1   IsPrize,
	dc.Id as centerid,
	s.Gln,
	p.kharazmi productcode -- پر کردن NULLها
	,N'mahya' as pakhsh, 47000 as  [valuepakhsh]
	, sku.id batchid
	--select * 
FROM web.Mahya_CustomerSales s
JOIN dw.kharazmi.DimCenterPakhsh dc ON dc.mahya = s.CenterCode
join (select * from web.mapproduct) p on p.mahya= s.ProductCode
 join (select * from dw.kharazmi.dimsku)sku on sku.productcode=  p.kharazmi and sku.batchno= ''
join(select * from price) p1 on  p1.ProductCode= s.ProductCode and p1.ShomarehFaktor= s.ShomarehFaktor and  p1.CenterCode=s.CenterCode
and p1.NoeBarge= s.NoeBarge

WHERE s.NoeBarge = N'فروش'
and Bonus&lt;&gt;0
and IsInsert=1</property>
