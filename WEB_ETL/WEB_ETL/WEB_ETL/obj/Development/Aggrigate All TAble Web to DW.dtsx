                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
2 as type,
s.FactorDate
    ,s.FactorID,
    abs (s.Quantity) as ReturnQuantity ,
    abs (s.Price) AS ReturnPrice,
    0 IsPrize,
    sku.id batchID,
    dc.Id as centerid,
    s.GLN,
    p.kharazmi productcode -- پر کردن NULLها
	,N'Qhasem' as pakhsh, 29000 as  [valuepakhsh]
    --s.ProductId
	--select * 
FROM web.Qhasem_CustomerSales  s
JOIN (select * from dw.kharazmi.DimCenterPakhsh) dc ON dc.Qasem = s.CenterCode
 join (select * from web.mapproduct) p on p.qasem= s.ProductCode
 join (select * from dw.kharazmi.dimsku)sku on sku.productcode=  p.kharazmi and sku.batchno= s.BatchNumber

WHERE RecordType like N'فاکتور مرجوعي'
and IsInsert=1
and IsPrize=0


union all 

SELECT 
2 as type,
s.FactorDate
    ,s.FactorID,
    abs (s.Quantity) as ReturnQuantity ,
    abs (s.Price) AS ReturnPrice,
     1 IsPrize,
    sku.id batchID,
