                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
    1 type,
    0 isprize,
    null invoiceNo,
    s.[Date] invoiceDate,
    c.Id centerid,
    p.kharazmi productcode,
    s.ProductCode ProductCodeOrig,
    null GLN,
    s.QuantitySold Quantity,
    sku.Id batchID,
    cast(s.SalesAmount / nullif(s.QuantitySold,0) as decimal(18,0)) Price,
    N'Nokhbegan' pakhsh,
	69219 as valuepakhsh
from stage.Web.Nokhbegan s
 join (select distinct kharazmi,nokhbegan from web.mapproduct) p 
    on p.nokhbegan = s.ProductCode
join (select * from dw.kharazmi.dimsku)sku on sku.productcode=  p.kharazmi and sku.batchno= ''
 join dw.kharazmi.DimCenterPakhsh c 
    on c.NOKHBEGAN = s.centercode
where 
s.isinsert = 1and
     s.QuantitySold &lt;&gt; 0
    ---and p.kharazmi is null;</property>
