, N'hejrat' as pakhsh
, 27000 as valuepakhsh
--select *
from stage.Web.Hejrat_CustomerSales s 
join (select * from web.mapproduct) p on p.hejrat= s.ProductCode
join dw.kharazmi.DimCenterPakhsh c 
on c.hejrat= s.centercode
join (select * from dw.kharazmi.dimsku)sku on sku.productcode=  p.kharazmi and sku.batchno= ''

where 
DocumentDescription = N'برگشت از فروش'
and  isinsert=1
and TotalSaleFee &lt;&gt; 0

union all

select 
1 as type
,1 as isprize
,null invoiceNo
,date invoiceDate 
, c.Id as centerid
, p.kharazmi productcode
, GLN
,abs( Quantity)Quantity
,sku.Id batchID 
,cast (BuyFee*abs (Quantity) as decimal (18,0) ) as Price
, N'hejrat' as pakhsh
, 27000 as valuepakhsh
--select *
from stage.Web.Hejrat_CustomerSales s 
join (select * from web.mapproduct) p on p.hejrat= s.ProductCode
join dw.kharazmi.DimCenterPakhsh c 
on c.hejrat= s.centercode
join (select * from dw.kharazmi.dimsku)sku on sku.productcode=  p.kharazmi and sku.batchno= ''

where 
DocumentDescription = N'برگشت از فروش'
and  isinsert=1
and TotalSaleFee = 0</property>
