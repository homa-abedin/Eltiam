                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--WITH ValidGTINs AS (
--    SELECT ProductID, MIN(GTIN) AS Valid_GTIN
--    FROM web.Daya_CustomerSales
--    WHERE GTIN IS NOT NULL
--    GROUP BY ProductID
--)

SELECT 
2 as type,
    s.Factordate,
    s.Quantity,
    s.Price  ,
    s.IsPrize,
      sku.id batchID,
    dc.Id as centerid,
    s.Gln,
    p.kharazmi productcode -- پر کردن NULLها
	,N'Daya' as pakhsh,69238 as  [valuepakhsh]
	--select*
FROM web.Daya_CustomerSales s
JOIN dw.kharazmi.DimCenterPakhsh dc ON dc.Daya = s.CenterId
left  join (select   distinct daya , kharazmi  from web.mapproduct) p on p.daya= s.ProductId
join (select * from dw.kharazmi.dimsku)sku on sku.productcode=  p.kharazmi and sku.batchno= s.BatchNumber

--where s.ProductName is null
WHERE s.RecordTypeId =2
and IsInsert=1</property>
